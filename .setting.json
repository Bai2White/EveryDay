{
	"window.openFilesInNewWindow": "on",
	"terminal.integrated.fontFamily": "FiraCode Nerd Font, FiraCodeNerdFontComplete-Retina, Meslo LG M DZ for Powerline",
	// 配置emmet
	"emmet.syntaxProfiles": {
		"vue-html": "html",
		"vue": "html"
	},
	"emmet.includeLanguages": {
		"vue-html": "html",
		"wxml": "html"
	},
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.triggerExpansionOnTab": true,
	// "emmet.showExpandedAbbreviation": "always",
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"terminal.external.osxExec": "iTerm.app",
	"gitlens.advanced.messages": {
		"suppressGitDisabledWarning": true,
		"suppressShowKeyBindingsNotice": true,
		"suppressWelcomeNotice": true
	},
	"git.autofetch": true,
	"gitlens.keymap": "alternate",
	"html.suggest.html5": true,
	"files.associations": {
		"*.nj": "html",
		"*.wpy": "vue",
		".gpmrc": "json",
		"*.cjson": "jsonc",
		"*.wxss": "css",
		"*.wxs": "javascript",
		"*.jxa": "jxa"
	},
	"editor.fontFamily": "FiraCode Nerd Font, FiraCodeNerdFontComplete-Retina, Menlo, Monaco, 'Courier New', 'MonteCarlo Fixed 12',monospace",
	"editor.fontLigatures": true,
	"markdown.preview.fontFamily": "FiraCode Nerd Font, FiraCodeNerdFontComplete-Retina, -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
	// 控制新行
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"editor.quickSuggestions": {
		"strings": true
	},
	"editor.formatOnPaste": false,
	"editor.formatOnSave": false,
	"editor.formatOnType": false,
	// 格式化css csscomb zen yandex
	"csscomb.preset": "zen",
	"gitProjectManager.baseProjectsFolders": [
		"/Applications/workPro/github",
	],
	"breadcrumbs.enabled": true,
	"editor.tabCompletion": "on",
	"editor.suggest.localityBonus": true,
	"workbench.startupEditor": "none",
	"editor.cursorSmoothCaretAnimation": true,
	"editor.smoothScrolling": true,
	"terminal.integrated.cursorBlinking": true,
	"colorize.colorized_variables": [
		"CSS",
		"SASS",
		"LESS",
		"STYLUS",
	],
	"workbench.sideBar.location": "right",
	"multiCommand.commands": [{
		"command": "multiCommand.makeRoom",
		"sequence": [
			"workbench.action.toggleSidebarVisibility",
			"workbench.action.toggleActivityBarVisibility"
		]
	}
],
	"npm.enableScriptExplorer": true,
	"bracket-pair-colorizer-2.showBracketsInGutter": true,
	"codemetrics.basics.DecorationModeEnabled": false,
	"search.showLineNumbers": true,
	"css.lint.duplicateProperties": "warning",
	"css.lint.importStatement": "error",
	"less.lint.duplicateProperties": "error",
	"less.lint.importStatement": "warning",
	"scss.lint.duplicateProperties": "warning",
	"scss.lint.importStatement": "warning",
	"editor.fontSize": 17,
	"showGitMetrics": true,
	"debug.console.fontFamily": "FiraCode Nerd Font, FiraCodeNerdFontComplete-Retina, Meslo LG M DZ for Powerline",
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"editor.colorDecorators": false,
	"highlight.regexes": {
		"(// ?TODO:?)(.*)": [{
				"overviewRulerColor": "#ffcc00",
				"backgroundColor": "#ffcc00",
				"color": "#1f1f1f",
				"fontWeight": "bold"
			},
			{
				"backgroundColor": "#d9ad00",
				"color": "#1f1f1f"
			}
		],
		"(<!-- ?TODO:?)(.*)": [{
				"overviewRulerColor": "#ffcc00",
				"backgroundColor": "#ffcc00",
				"color": "#1f1f1f",
				"fontWeight": "bold"
			},
			{
				"backgroundColor": "#d9ad00",
				"color": "#1f1f1f"
			}
		],
		"(<!-- ?FIXME:?)(.*)": [{
				"overviewRulerColor": "#ff0000",
				"backgroundColor": "#ff0000",
				"color": "#1f1f1f",
				"fontWeight": "bold"
			},
			{
				"backgroundColor": "#d90000",
				"color": "#1f1f1f"
			}
		],
		"(// ?FIXME:?)(.*)": [{
				"overviewRulerColor": "#ff0000",
				"backgroundColor": "#ff0000",
				"color": "#1f1f1f",
				"fontWeight": "bold"
			},
			{
				"backgroundColor": "#d90000",
				"color": "#1f1f1f"
			}
		],
		"(// )(@\\w+)": [{},
			{
				"color": "#4de0ff"
			}
		],
		"(//)(//\\w+)": [{
				"color": "#474747",
				"textDecoration": "line-through",
			},
			{
				"color": "#474747",
				"textDecoration": "line-through",
			}
		]
	},
	"minapp-vscode.disableAutoConfig": true,
	"code-runner.executorMap": {
		"typescript": "node_modules/.bin/ts-node",
	},
	"px-to-rpx.baseWidth": 375,
	"[html]": {
		"editor.defaultFormatter": "HookyQR.beautify"
	},
	// 设置清理css等级
	"CleanCSS.options": {
		"level": 2
	},
	// 设置todo-tree标签
	"todo-tree.general.tags": [
		"TODO",
		"FIXME"
	],
	"dimmer.enabled": true,
	"terminal.integrated.shell.osx": "/bin/zsh",
	"cssrem.rootFontSize": 20,
	"cssrem.fixedDigits": 2,
	"workbench.editor.highlightModifiedTabs": true,
	"vim.easymotion": true,
	"vim.sneak": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.insertModeKeyBindings": [{
		"before": ["j", "j"],
		"after": ["<Esc>"]
	}],
	"vim.normalModeKeyBindingsNonRecursive": [{
			"before": ["<leader>", "d"],
			"after": ["d", "d"]
		},
		{
			"before": ["<C-n>"],
			"commands": [":nohl"]
		}
	],
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false
	},
	"editor.renderWhitespace": "none",
	"editor.renderControlCharacters": false,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"powermode.enabled": true,
	"powermode.enableShake": false,
	"powermode.presets": "simple-rift",
	"powermode.enableStatusBarComboCounter": false,
	"powermode.enableStatusBarComboTimer": false,
	"todo-tree.tree.showScanModeButton": false,
	// "workbench.colorCustomizations": {
	//   "editor.selectionBackground": "#135564" // 代码选中颜色
	// },
	"files.useExperimentalFileWatcher": true, // 解决cpu占用过高问题
	"files.exclude": {
		"/.git": true,
		"/.DS_Store": true,
		"/node_modules": true,
		"/node_modules/": true,
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/tmp": true,
		"**/node_modules": true,
		"**/bower_components": true,
		"**/dist": true // 解决cpu占用过高问题
	},
	"search.exclude": {
		"/node_modules": true,
		"**/node_modules": true,
	},
	"files.watcherExclude": {
		"/node_modules/": true,
		"**/node_modules": true,
		"**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/tmp/**": true,
        "**/bower_components/**": true,
		"**/dist/**": true,
        "**/vendor/**": true,
        "assets/**": true,
        "backup/**": true,
        "bin/**": true,
        "cache/**": true,
        "images/**": true,
        "logs/**": true,
        "system/**": true,
        "tmp/**": true,
        "webserver-configs/**": true // 解决cpu占用过高问题
	},
	"workbench.iconTheme": "vscode-icons",
	"rest-client.fontSize": 17,
	"terminal.integrated.fontSize": 17,
	"debug.console.fontSize": 17,
	// 抗锯齿 待定
	"workbench.fontAliasing": "auto",
	"rest-client.fontFamily": "FiraCode Nerd Font",
	// 斜体设置 待定
	"editor.tokenColorCustomizations": {
		"textMateRules": [{
				"scope": [
					//following will be in italic (=FlottFlott)
					"comment",
					"entity.name.type.class", //class names
					"keyword", //import, export, return…
					"constant", //String, Number, Boolean…, this, super
					"storage.modifier", //static keyword
					"storage.type.class.js", //class keyword
				],
				"settings": {
					"fontStyle": "italic"
				}
			},
			{
				"scope": [
					//following will be excluded from italics (VSCode has some defaults for italics)
					"invalid",
					"keyword.operator",
					"constant.numeric.css",
					"keyword.other.unit.px.css",
					"constant.numeric.decimal.js",
					"constant.numeric.json"
				],
				"settings": {
					"fontStyle": ""
				}
			}
		]
	},
